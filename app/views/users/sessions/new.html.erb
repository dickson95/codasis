<div class="panel panel-default devise-bs">
    <div class="panel-heading">
      <h4><center><b><%= t('.ingresar', :default => "INGRESAR") %></b></center></h4>
    </div>
   </div> 
    <div class="col-md-offset-4 col-md-4">
      <div class="panel-body">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { role: "form" }) do |f| %>
          <div class="form-group">
            <%= f.label :Usuario %>
            <%= f.email_field :email, autofocus: true, class: "form-control" %>
          </div>
          <div class="form-group">
            <%= f.label :Contraseña %>
            <%= f.password_field :password, autocomplete: "off", class: "form-control" %>
          </div>
          <% if devise_mapping.rememberable? %>
            <div class="checkbox">
              <%= f.label :Recuérdame do %>
                <%= f.check_box :remember_me %><%= t('.remember_me', :default => "Recuérdame") %>
              <% end %>
            </div>
          <% end %>
          <%= f.submit  t('.Ingresar', :default => "Aceptar"), class: "btn btn-primary" %>
          &nbsp; &nbsp; <%= link_to '<i class="fa fa-reply fa-2x" title="ATRAS"></i>'.html_safe, root_path %>
        <% end %>
      </div>
    </div>  
</div>
<%= render "devise/shared/links" %>
<%#ok%>